{
  "startCommand": "npm run start",
  "url": "http://localhost:8080",
  "targets": [
    "chromium",
    "firefox"
  ],
  "viewport": {
    "width": null,
    "height": null
  },
  "titleBasedScreenshots": true,
  "parallelTests": 5,
  "defaultTimeout": 25000,
  "tolerance": 2.5,
  "antialiasingTolerance": 3.5,
  "coverageExclude": [
    "/node_modules/**",
    "/webpack/**",
    "/ws '('ignored')'",
    "/'('webpack')'/**",
    "/'('webpack')'-dev-server/**"
  ]
}